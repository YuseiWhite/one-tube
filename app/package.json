{
  "name": "one-tube-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch src/server/server.ts",
    "server:dev": "DOTENV_CONFIG_PATH=../.env tsx src/server/server.ts",
    "build": "npm run build:server && vite build",
    "build:server": "tsc --project tsconfig.node.json",
    "preview": "vite preview",
    "start": "node dist-server/server/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "@mysten/dapp-kit": "^0.14.26",
    "@mysten/sui": "^1.14.0",
    "@tanstack/react-query": "^5.62.11",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "dotenv": "^17.2.3",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
